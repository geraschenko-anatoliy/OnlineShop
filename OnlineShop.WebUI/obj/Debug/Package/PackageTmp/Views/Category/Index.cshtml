@model IEnumerable<OnlineShop.Domain.Entities.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@foreach (var item in Model)
{
    <table>
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Edit", new { id = item.CategoryID })
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new { id = item.CategoryID})
            </td>
        </tr>
    </table>
}
